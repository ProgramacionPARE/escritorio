	<queryString><![CDATA[SELECT SD.progresivo,SD.fecha_proximo_seguimiento,SD.fecha_compromiso,SD.lugar_proximo_seguimiento,SD.descripcion_falta,SD.causa_empleado,SD.acciones_a_tomar,
	 lst0.nombre AS CENTRO_OPER,lst1.nombre AS CENTRO_COSTOS,SD.ap_paterno,SD.ap_materno,SD.nombres,
	 SD.codigo_empleado,lst2.nombre AS PUESTO,lst3.nombre AS DEPTO 
 FROM tbl_seguimiento_disciplinario SD 

LEFT JOIN listas lst0 ON SD.id_centro_operativo=lst0.elemento AND lst0.lista=2 AND lst0.elemento>0 #2=centro_operativo
LEFT JOIN listas lst1 ON SD.id_centro_costos=lst1.elemento AND lst1.lista=1 AND lst1.elemento>0 #1=Centro_costos
LEFT JOIN listas lst2 ON SD.id_puesto=lst2.elemento AND lst2.lista=11 AND lst2.elemento>0 #11=PUESTOS
LEFT JOIN listas lst3 ON SD.id_departamento=lst3.elemento AND lst3.lista=10 AND lst3.elemento>0 #1=DEPARTAMENTOS

WHERE SD.progresivo=$P{PARAM_PROGRESIVO}]]></queryString>

