	<queryString><![CDATA[SELECT OP.id,OP.progresivo,CC.nombre AS CENTRO_COSTOS,lst0.nombre AS CENTRO_OPER,lst1.nombre AS CASETA, 
	turnos.descripcion AS TURNO, lst2.nombre AS CAJERO, OP.nombre_propietario AS NOMBRE, 
	OP.telefono_propietario AS TELEFONO, lst3.nombre AS IDENTIFICACION, OP.monto_adeudo AS MONTO, 
	OP.num_boleto FROM fto_objetos_prenda OP 

LEFT JOIN listas lst0 ON OP.id_centro_operativo = lst0.elemento AND lst0.lista = 2 AND lst0.elemento > 0 # 2 = Centro_operativo
LEFT JOIN listas CC ON OP.id_centro_costos = CC.elemento AND CC.lista = 1 AND CC.elemento > 0 # 2 = Centro_costos
LEFT JOIN listas lst1 ON OP.id_caseta = lst1.elemento AND lst1.lista = 3 AND lst1.elemento > 0 # 3 = Casetas
LEFT JOIN turnos ON turnos.id = OP.id_turno
LEFT JOIN listas lst2 ON OP.id_cajero = lst2.elemento AND lst2.lista = 5 AND lst2.elemento > 0	#  5 = Cajeros
LEFT JOIN listas lst3 ON OP.id_identificacion = lst3.elemento AND lst3.lista = 6 AND lst2.elemento > 0	# 6 = Identificacion
WHERE OP.progresivo = $P{PARAM_PROGRESIVO_OP}]]></queryString>

