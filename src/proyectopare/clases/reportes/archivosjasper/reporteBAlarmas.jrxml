	<parameter name="PARAM_FOLIO" isForPrompting="true" class="java.lang.Long">
		<defaultValueExpression ><![CDATA[new java.lang.Long(1)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT BP.id AS FOLIO, lst0.nombre AS CENTRO_OPER, lst1.nombre AS CASETA, turnos.descripcion AS TURNO, lst2.nombre AS CAJERO, BP.nombre_propietario AS NOMBRE,BP.domicilio_propietario AS DIRECCION, BP.telefono_propietario AS TELEFONO,
		lst3.nombre AS IDENTIFICACION, lst4.nombre AS MARCA, BP.modelo AS MODELO, lst5.nombre AS COLOR, BP.matricula AS PLACAS, 
		BP.num_tarjeta_circulacion AS TARJETA_CIRCULACION FROM fto_boleto_perdido BP  

LEFT JOIN listas lst0 ON BP.id_centro_operativo = lst0.elemento AND lst0.lista = 2 AND lst0.elemento > 0 # 2 = Centro_operativo
LEFT JOIN listas lst1 ON BP.id_caseta = lst1.elemento AND lst1.lista = 3 AND lst1.elemento > 0 # 3 = Casetas
LEFT JOIN turnos ON turnos.id = BP.id_turno
LEFT JOIN listas lst2 ON BP.id_cajero = lst2.elemento AND lst2.lista = 5 AND lst2.elemento > 0	#  5 = Cajeros
LEFT JOIN listas lst3 ON BP.id_identificacion = lst3.elemento AND lst3.lista = 6 AND lst2.elemento > 0	# 6 = Identificacion
LEFT JOIN listas lst4 ON BP.id_marca = lst4.elemento AND lst4.lista = 7 AND lst2.elemento > 0 # 7 = Marcas
LEFT JOIN listas lst5 ON BP.id_color = lst5.elemento AND lst5.lista = 8 AND lst2.elemento > 0 # 8 = Colores
WHERE BP.id = $P{PARAM_FOLIO}]]></queryString>

