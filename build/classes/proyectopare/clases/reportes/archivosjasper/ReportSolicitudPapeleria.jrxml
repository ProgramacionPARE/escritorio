	<parameter name="PARAM_PROGRESIVO" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT SP.progresivo, lst2.nombre AS CENTRO_COSTOS, SP.num_formato, DATE_FORMAT(SP.fecha,'%d-%m-%Y') AS fecha, lst3.nombre AS CENTRO_OPERATIVO, SP.articulos, SP.cantidad, SP.del_folio, SP.al_folio, SP.observaciones
FROM tbl_solicitud_papeleria SP
LEFT JOIN listas lst2 ON SP.id_centro_costos=lst2.elemento AND lst2.lista='1' AND lst2.elemento>0 
LEFT JOIN listas lst3 ON SP.id_centro_operativo=lst3.elemento AND lst3.lista='2' AND lst3.elemento>0 
WHERE SP.progresivo=$P{PARAM_PROGRESIVO}]]></queryString>

